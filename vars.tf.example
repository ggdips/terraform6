variable "vs_token" {
  default  = "Put your token here"
}

variable "aws_acckey" {
  default  = "Put your AWS access key here"
}

variable "aws_skey" {
  default  = "Put your AWS secret key here"
}

variable "aws_rb_zoneid" {
  default  = "Put your AWS zone ID here"
}

variable "vscale_msk" {
  description = "vscale MSK data"
  default     = "msk0"
}

variable "vscale_centos_7" {
  description = "centos"
  default     = "centos_7_64_001_master"
}

variable "vscale_rplan" {
  type = "map"
  default = {
    "s"   = "small"
    "m"   = "medium"
    "l"   = "large"
    "xl"  = "huge"
    "xxl" = "monster"
  }
}

variable "srvtemplname" {
  default = "hostnameprefix-"
}

variable "srvcount" {
  type = number
  default = 2
}
